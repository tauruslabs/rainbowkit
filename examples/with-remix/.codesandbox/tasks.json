{
  "$schema": "https://codesandbox.io/schemas/tasks.json",
  "setupTasks": ["yarn add @merkletrade/rainbowkit@latest", "yarn build"],
  "tasks": {
    "install-dependencies": {
      "name": "Install Dependencies",
      "command": "yarn add @merkletrade/rainbowkit@latest"
    },
    "start-app": {
      "name": "Run Dev Server",
      "command": "yarn dev",
      "runAtStart": true,
      "preview": {
        "port": 8000
      }
    }
  }
}